generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id            Int     @id @default(autoincrement())
  First_name    String
  Last_Name     String
  Password      String
  Email_address String  @unique
  Is_Youtuber   Boolean
  Is_Pro        Boolean
}
